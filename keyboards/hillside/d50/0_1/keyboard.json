{
    "bootloader": "rp2040",
    "build": {
        "lto": true
    },
    "caps_word": {
        "enabled": true
    },
    "development_board": "promicro_rp2040",
    "diode_direction": "COL2ROW",
    "features": {
        "extrakey": true,
        "rgblight": true
    },
    "matrix_pins": {
        "cols": ["GP27", "GP26", "GP22", "GP20", "GP23", "GP21"],
        "rows": ["GP0", "GP5", "GP6", "GP8", "GP9"]
    },
    "pin_compatible": "promicro",
    "processor": "RP2040",
    "rgblight": {
        "animations": {
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true
        },
        "brightness_steps": 8,
        "hue_steps": 8,
        "led_count": 4,
        "saturation_steps": 8,
        "sleep": true,
        "split": true,
        "split_count": [2, 2]
    },
    "split": {
        "enabled": true,
        "serial": {
            "driver": "vendor",
        }
    },
    "usb": {
        "device_version": "0.0.1",
        "pid": "0x4850",
        "vid": "0x4D4D"
    },
    "ws2812": {
        "driver": "vendor",
        "pin": "GP7"
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"label": "`~", "matrix": [0, 0], "x": 0, "y": 0, "hand": "L"},
                {"label": "Q", "matrix": [0, 1], "x": 1, "y": 0, "hand": "L"},
                {"label": "W", "matrix": [0, 2], "x": 2.1, "y": 0},
                {"label": "E", "matrix": [0, 3], "x": 3.2, "y": 0},
